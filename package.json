{
  "name": "@pureadmin/table",
  "version": "1.2.0",
  "packageManager": "pnpm@7.0.1",
  "description": "Use tsx syntax to encapsulate element-plus Table twice to provide flexible configuration items",
  "keywords": [
    "@pureadmin/table",
    "element-plus",
    "typescript",
    "vue3",
    "vite",
    "tsx"
  ],
  "homepage": "https://github.com/xiaoxian521/pure-admin-table/tree/main/#readme",
  "bugs": {
    "url": "https://github.com/xiaoxian521/pure-admin-table/issues"
  },
  "license": "MIT",
  "author": "xiaoxian521",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoxian521/pure-admin-table"
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.umd.js",
  "jsdelivr": "dist/index.umd.js",
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.es.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "volar.d.ts"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "test": "vitest run",
    "build": "vite build",
    "preview": "vite preview",
    "lib": "vite build && vue-tsc -p tsconfig.build.json",
    "lint": "prettier --write  \"packages/**/*.{ts,tsx}\" \"__tests__/table.test.tsx\"",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "element-plus": "^2.0.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "webpack"
      ]
    }
  },
  "dependencies": {
    "element-plus": "^2.2.6",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@esbuild-kit/cjs-loader": "^2.2.0",
    "@types/node": "^18.0.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/test-utils": "^2.0.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.2",
    "prettier": "^2.7.1",
    "resize-observer-polyfill": "^1.5.1",
    "rollup": "^2.75.7",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.54.3",
    "sass-loader": "^13.0.2",
    "typescript": "^4.7.4",
    "vite": "^2.9.12",
    "vite-svg-loader": "^3.4.0",
    "vitest": "^0.16.0",
    "vue-tsc": "^0.38.1"
  }
}
